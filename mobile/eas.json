{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "releaseChannel": "production",
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "channel": "production"
    },
    "testing": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "testing"
    }
  }
}
