{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "hooks": {
       "pre-install": "bash eas-build/pre-install.sh"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON_BASE64": "$GOOGLE_SERVICES_JSON_BASE64"
      }
    },
    "testing": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "testing"
    }
  }
}
